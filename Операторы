from random import randint
a=randint(0,100)
b=randint(0,100)
c=randint(0,100)
print(a,b,c)
print((a+b+c)//3)

from random import randint
a=randint(0,100)
b=randint(0,100)
print(a,b)
print(a//b)  #целочисленного деления этих чисел
print(a%b)  #остаток  от деления

amount = float(input())
print(round(amount))
print(round(amount,2))
print('{:016}'.format(amount))